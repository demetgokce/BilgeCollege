@model OgretmenDers


<div class="row">
    <div class="col-md-10">
        <form method="post" asp-controller="OgretmenDers" asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="OgretmenId" class="control-label"></label>
                <select asp-for="OgretmenId" class="form-control" asp-items="ViewBag.OgretmenId"></select>
            </div>
            <div class="form-group">
                <label asp-for="DersId" class="control-label"></label>
                <select asp-for="DersId" class="form-control" asp-items="ViewBag.DersId"></select>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
            <div>
                <a asp-action="Index" class="btn btn-primary">Listeye Dön</a>
            </div>
        </form>
    </div>
</div>
