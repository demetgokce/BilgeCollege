@model OgrenciSiniflar

<div class="row">
    <div class="col-md-10">
        <form method="post" asp-controller="OgrenciSiniflar" asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="SiniflarId" class="control-label"></label>
                <select asp-for="SiniflarId" class="form-control" asp-items="ViewBag.SiniflarId"></select>
            </div>
            <div class="form-group">
                <label asp-for="OgrenciId" class="control-label"></label>
                <select asp-for="OgrenciId" class="form-control" asp-items="ViewBag.OgrenciId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
            <div>
                <a asp-action="Index" class="btn btn-primary">Listeye Dön</a>
            </div>
        </form>
    </div>
</div>
